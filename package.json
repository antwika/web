    "release": "standard-version"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "jose": "^4.8.1",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-intl": "5.25.1",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.0.1",
    "@types/node": "17.0.30",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/react-intl": "3.0.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.6",
    "husky": "7.0.4",
    "standard-version": "9.3.2",
    "typescript": "4.6.4"
  },
