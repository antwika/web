{
  "license": "MIT",
  "name": "web",
  "version": "0.0.3",
  "private": false,
  "scripts": {
    "prepare": "husky install && yarn precompile",
    "precompile": "yarn precompile:locales",
    "precompile:locales": "formatjs compile-folder --ast --format simple content/locales content/compiled-locales",
    "dev": "yarn precompile && next dev",
    "build": "yarn precompile && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "cypress run --component",
    "release": "standard-version"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "jose": "^4.8.1",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-intl": "6.0.4",
    "react-redux": "^8.0.1",
    "react-router-dom": "6.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@formatjs/cli": "^5.0.1",
    "@types/node": "17.0.30",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/react-intl": "3.0.0",
    "@types/uuid": "^8.3.4",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^10.1.0",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "husky": "8.0.1",
    "standard-version": "9.5.0",
    "typescript": "4.7.3"
  },
  "repository": "git@github.com:antwika/web.git",
  "bugs": {
    "url": "https://github.com/antwika/web/issues"
  },
